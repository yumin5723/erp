{% set sidebar_name = "敏感词" %}
{% set menu_name = "keyword" %}
{% include "../layouts/header.twig" %}
<div class="content">
<h1>管理员信息</h1>
{% block content %}
<div class="detail-grid-view" id="log-panel-detailed-grid">
    <div class="summary">Showing <b>1-1</b> of <b>{{ count }}</b> item.</div>
        <table class="table table-striped table-bordered">
            <thead>
                <tr><th>Id</th><th>敏感词</th><th>操作</th></tr>
            </thead>
            <tbody>
                {% for  key,value in data %}
                        <tr data-key="1"><td>{{ key }}</td><td>{{ value }}</td><td><a href="/keyword/delete?word={{ value }}" class="delete" onclick="return confirm(&quot;是否将此用户信息删除?&quot;)">delete</a></td></tr>
                {% endfor %}
            </tbody>
        </table>
<ul class="pagination"><li class="prev disabled"><span>«</span></li>
<li class="active"><a data-page="0" href="/keyword/admin?page=1">1</a></li>
<li class="next disabled"><span>»</span></li></ul></div>
{% endblock %}
</div>