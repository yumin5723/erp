{% extends "layouts/base.twig" %}
{% set menu_name = "专题页面" %}
{% block mainbody %}
<div id="sidebar-nav">
  {% block sidebar %}
        <ul id="dashboard-menu" class="nav nav-list">
           {% for item in this.sidebars %}
           <li class=" theme-mobile-hack {% if sidebar_name == item.id %}cur{% endif %}"><a href="{{ this.createUrl(item.url) }}"><i class="icon-{{ item.icon }}"></i> <span>{{ item.name }}</span></a></li>
           {% endfor %}
        </ul>
    {% endblock %}
</div>
<div class="content">
  {% block content %}
  {% endblock %}
</div>
{% endblock %}